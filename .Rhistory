##Used to get the cache data

cacheSolve <- function(x, ...) ##gets cache data
  {
  inv<-x$getinv()
  if(!is.null(inv)){
                    messege("getting cached data!")
                    return(inv)       #Return inverse value
  }
  data<-x$get()
  inv<-solve(data,...)
  x$setinv(inv)
  inv   ##Return a matrix that is the inverse of 'x'
}
}
